{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "dependencies": {
        "@ant-design/icons": "^5.3.7",
        "@fortawesome/fontawesome-free": "^6.5.2",
        "@reduxjs/toolkit": "^2.2.3",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "antd": "^5.16.2",
        "axios": "^1.6.8",
        "chart.js": "^4.4.2",
        "lodash": "^4.17.21",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-chartjs-2": "^5.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.1.0",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.22.3",
        "react-scripts": "5.0.1",
        "sass": "^1.77.4",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "CI=false react-scripts build",
        "test": "react-scripts test --coverage",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "jest": {
        "coverageThreshold": {
            "global": {
                "branches": 2,
                "functions": 2,
                "lines": 2,
                "statements": 2
            }
        },
        "transform": {
            "^.+\\.[t|j]sx?$": "babel-jest"
        },
        "coverageReporters": [
            "cobertura",
            "json",
            "lcov",
            "text"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}",
            "!src/**/*.test.{js,jsx}",
            "!src/index.js",
            "!src/reportWebVitals.js",
            "!src/setupTests.js"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.24.5",
        "@babel/preset-env": "^7.24.5",
        "@babel/preset-react": "^7.24.1",
        "@testing-library/jest-dom": "^6.4.5",
        "@testing-library/react": "^15.0.7",
        "@testing-library/user-event": "^14.5.2",
        "chai": "^5.1.0",
        "react-test-renderer": "^18.3.1",
        "redux-mock-store": "^1.5.4",
        "redux-thunk": "^3.1.0",
        "sinon": "^17.0.1"
    }
}
